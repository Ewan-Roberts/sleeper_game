{
  "name": "mmo",
  "version": "1.0.0",
  "description": "socket game",
  "main": "app.js",
  "scripts": {
    "watch": "watchify front_end/index.js -o ./public/js/bundle.js",
    "build": "uglifyjs ./public/js/bundle.js --output ./public/js/bundle.js --compress --mangle",
    "start": "nodemon server",
    "spritesheet": "pixi-packer ./scripts/images.js public/images/ --clean-cache",
    "test": "mocha --recursive",
    "count": "cloc --exclude-dir=node_modules ."
  },
  "eslint.options": {
    "configFile": "./.eslintrc.json"
  },
  "author": "Ewan",
  "license": "ISC",
  "dependencies": {
    "chai": "^4.2.0",
    "easystarjs": "^0.4.3",
    "express": "^4.16.3",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^6.0.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.2.15",
    "password-generator": "^2.2.0",
    "pixi-layers": "^0.1.9",
    "pixi-packer-parser": "^2.0.0",
    "pixi-shadows": "0.0.7",
    "pixi-timer": "^1.0.1",
    "pixi-tween": "^0.2.0",
    "pixi.js": "^4.8.5",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^4.0.0"
  }
}
