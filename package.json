{
  "name": "mmo",
  "version": "1.1.8",
  "description": "Pixi.js web game",
  "scripts": {
    "watch": "watchify web/index.js -o ./public/js/bundle.js -v",
    "build": "uglifyjs ./public/js/bundle.js --output ./public/js/bundle.js --compress --mangle",
    "start": "nodemon server",
    "rinse": "sh ./scripts/clean_assets.sh",
    "trim": "node ./scripts/crop2.js",
    "spritesheet": "pixi-packer  ./scripts/image_compression.js public/images/ --clean-cache",
    "test": "mocha"
  },
  "eslint.options": {
    "configFile": "./.eslintrc.json"
  },
  "author": "Ewan Roberts",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "10.0.2",
    "browserfy": "1.0.0",
    "chai": "^4.2.0",
    "compression": "1.7.3",
    "easystarjs": "0.4.3",
    "express": "4.16.3",
    "imagemin-jpegtran": "6.0.0",
    "imagemin-pngquant": "6.0.0",
    "jimp": "0.6.1",
    "pixi-keyboard": "0.9.4",
    "pixi-packer-parser": "2.0.0",
    "pixi-sound": "2.1.0",
    "pixi-tween": "0.2.0",
    "pixi-viewport": "4.0.1",
    "pixi.js": "4.8.8",
    "png-js": "0.1.1",
    "worker-timers": "5.0.19"
  },
  "devDependencies": {
    "canvas": "^2.6.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "gulp": "^4.0.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "3.0.2"
  }
}
